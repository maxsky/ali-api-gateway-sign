{
    "name": "maxsky/aliyun-api-gateway-sdk",
    "description": "Ali API Gateway Request SDK",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "nesbot/carbon": "^1.33|^2.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Aliyun\\ApiGateway\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "AliCloudApiGatewayTest\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
